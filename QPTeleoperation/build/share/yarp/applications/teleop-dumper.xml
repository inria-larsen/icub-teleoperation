 <!-- Data dumper iCub teleoperation -- (C) Luigi Penco -->

<application>
<name>iCub Teleoperation data dumper</name>
<dependencies>
	<port>/icub/right_arm/state:o</port>
	<port>/icub/left_arm/state:o</port>
	<port>/icub/right_leg/state:o</port>
	<port>/icub/left_leg/state:o</port>
	<port>/icub/torso/state:o</port>
	<port>/icub/head/state:o</port>
</dependencies>

    <!-- JOINTS -->
<module>
	<name>yarpdatadumper</name>
	<parameters>--name /dump_upperbody/head --type bottle --rxTime</parameters>
	<node>luigi</node>
	<stdio>luigi</stdio>
	<tag>head-joints</tag>
</module>
<module>
	<name>yarpdatadumper</name>
	<parameters>--name /dump_upperbody/torso --type bottle --rxTime</parameters>
	<node>luigi</node>
	<stdio>luigi</stdio>
	<tag>torso-joints</tag>
</module>
<module>
	<name>yarpdatadumper</name>
	<parameters>--name /dump_upperbody/leftArm --type bottle --rxTime</parameters>
	<node>luigi</node>
	<stdio>luigi</stdio>
	<tag>left-arm-joints</tag>
</module>
<module>
	<name>yarpdatadumper</name>
	<parameters>--name /dump_upperbody/rightArm --type bottle --rxTime</parameters>
	<node>luigi</node>
	<stdio>luigi</stdio>
	<tag>right-arm-joints</tag>
</module>
<module>
	<name>yarpdatadumper</name>
	<parameters>--name /dump_upperbody/leftLeg --type bottle --rxTime</parameters>
	<node>luigi</node>
	<stdio>luigi</stdio>
	<tag>left-leg-joints</tag>
</module>
<module>
	<name>yarpdatadumper</name>
	<parameters>--name /dump_upperbody/rightLeg --type bottle --rxTime</parameters>
	<node>luigi</node>
	<stdio>luigi</stdio>
	<tag>right-leg-joints</tag>
</module>
  

<!-- CONNECTIONS -->
<connection>
  <from>/icub/right_arm/state:o</from>
  <to>/dump_upperbody/rightArm</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/left_arm/state:o</from>
  <to>/dump_upperbody/leftArm</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/head/state:o</from>
  <to>/dump_upperbody/head</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/torso/state:o</from>
  <to>/dump_upperbody/torso</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/right_leg/state:o</from>
  <to>/dump_upperbody/rightLeg</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/left_leg/state:o</from>
  <to>/dump_upperbody/leftLeg</to>
  <protocol>udp</protocol>
</connection>

 
</application>
